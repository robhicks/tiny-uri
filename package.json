{
  "name": "tiny-uri",
  "version": "9.0.0",
  "description": "Lightweight Javascript library for handling URLs",
  "keywords": [
    "Lightweight",
    "Javascript",
    "library",
    "es2015",
    "URLs"
  ],
  "repository": "https://github.com/robhicks/tiny-uri.git",
  "browser": "dist/TinyUri.js",
  "main": "index.js",
  "module": "dist/tiny-uri.js",
  "scripts": {
    "build": "npx rollup -c",
    "docs": "npx esdoc -c .esdoc.json",
    "mocha": "mocha -u bdd -R spec -t 5000 --watch test/test-bundle.js",
    "postinstall": "run-s build docs",
    "start": "rollup -cw",
    "test": "mocha -u bdd -R spec -t 5000 test/test-bundle.js"
  },
  "author": "rob@hixfamily.org",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "rollup-plugin-live-server": "^2.0.0",
    "serialize-javascript": ">=5.0.1",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "static-server": "^2.2.1"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.45.2",
    "rollup-plugin-server": "^0.7.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
